    "bcrypt": "^5.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.0",
    "nodemon": "^3.0.0",
    "concurrently": "^8.2.0"
  }
}